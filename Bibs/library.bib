@misc{Asynchronie, hypothèse de la meilleure option, qualité individuelle, oiseau marin, fidélité,
abstract = {Chez les esp\`{e}ces long\'{e}vives et monogames dont les partenaires se s\'{e}parent entre deux saisons de re- production, attendre son ancien partenaire pour d\'{e}buter une reproduction serait une strat\'{e}gie adaptative. Nous nous sommes int\'{e}ress\'{e}s \`{a} un oiseau marin long\'{e}vif et monogame, le Manchot royal Aptenodytes patagonicus, gr\^{a}ce au suivi de 10 ans, automatis\'{e}, d’oiseaux transpond\'{e}s des \^{\i}les Crozet. Bien que mo- nogame sur une reproduction, on observe un fort taux de divorce interannuel (63\%), qui, cependant, est significativement plus faible que celui de la litt\'{e}rature. Cette diff\'{e}rence serait li\'{e}e \`{a} l’effet des bagues alaires sur la synchronie entre partenaires. Or, nos r\'{e}sultats montrent que la synchronie des retours joue un r\^{o}le fondamental dans le rappariement des partenaires, une asynchronie inf\'{e}rieure \`{a} 24h menant \`{a} un taux de divorce nul. Cependant, attendre son partenaire \`{a} un co\^{u}t (reproduction plus tardive donc succ\`{e}s plus faible, r\'{e}serves \'{e}nerg\'{e}tiques surnum\'{e}raires), et lorsque l’asynchronie d\'{e}passe 10 jours, les partenaires d’un couple ne se retrouvent jamais. Enfin, nous trouvons qu’un individu de meilleure qualit\'{e} divorcer moins souvent, et qu’il serait tr\`{e}s synchrone dans ses dates de retours, facilitant la synchroni- sation avec son partenaire. Cette h\'{e}t\'{e}rog\'{e}n\'{e}it\'{e} de qualit\'{e} individuelle au sein de la population supporte l’hypoth\`{e}se de la "meilleure option". Les forts taux de divorce du genre Aptenodytes pourrait \^{e}tre \'{e}gale- ment li\'{e}s \`{a} l’absence de nid, les co\^{u}ts d’une strat\'{e}gie de r\'{e}tention du partenaire serait donc trop \'{e}lev\'{e}s et une strat\'{e}gie de divorce optimal permettrait un meilleur ajustement de leur long cycle reproducteur aux changements saisonniers de leur environnement.},
address = {Rennes},
author = {Toscani, C},
booktitle = {Science},
file = {:C$\backslash$:/Users/Hedjour/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Toscani - 2010 - Science.pdf:pdf},
institution = {University of Rennes-1},
number = {Umr 7178},
pages = {30},
title = {{Le divorce chez le Manchot royal Apetnodytes patagonicus:}},
year = {2010}
}
