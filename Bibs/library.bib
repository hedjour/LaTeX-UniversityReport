@article{Parsons1976,
abstract = {(1) From 1966 to 1968, 14 395 herring gulls of known date of hatching were ringed on the Isle of May, Scotland, from a colony whose breeding season had been extended in 1967-68 by large scale egg removal followed by re-laying. Post-fledging survival in relation to date of hatching was analysed, not only by examining first winter recoveries, but also by studying large numbers of ringed birds that were culled between 1972 and 1975 when they returned to the island to breed. (2) There was no major difference in the proportions of young recovered up to 31 December in the year of hatching according to hatching date, although fewer recoveries of very late hatched young were reported. This result was not verified by the much larger sample of recoveries provided by the culls. (3) The proportions of young that survived to be culled showed that in the 1966 year class, only young hatching after 29 June survived less well than the remainder. This group represented only 3\% of the total sample. In the following year class, there was no relationship between hatching date and post-fledging survival, even amongst abnormally late hatching chicks from repeat clutches. However, post-fledging mortality of 1968 young appeared to be twice as high as in the previous two year classes, and the proportion recovered in the culls was inversely correlated with date of hatching. This indicated a progressively increased post-fledging mortality with lateness of hatching. (4) In 1968, but not in the previous two years, juveniles from broods of three fledged chicks survived less well than those from smaller broods. For those hatching later in the season, even juveniles from broods of two appeared to be at a disadvantage compared with those from broods of one. (5) Various reasons for the relationship between date of hatching and post-fledging survival are discussed.},
author = {Parsons, J. and Chabrzyk, G. and Duncan, N.},
file = {:D$\backslash$:/Hedjour/s.v/Master/M1/Manchots royaux-InvPar/Biblio/parentalcare/parentalcare/Parsons, Chabrzyk, Duncan - 1976 - Journal of Animal Ecology.pdf:pdf},
journal = {Journal of Animal Ecology},
number = {3},
pages = {667 -- 675},
title = {{Effects of Hatching Date on Post-Fledging Survival in Herring Gulls}},
url = {http://www.jstor.org/stable/3574},
volume = {45},
year = {1976}
}
