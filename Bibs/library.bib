@article{Hinde2010,
abstract = {The evolution of family life has traditionally been studied in parallel by behavioral ecologists and quantitative geneticists. The former focus on parent-offspring conflict and whether parents or offspring control provisioning, whereas the latter concentrate on the coadaptation of parental supply and offspring demand. Here we show how prenatal effects on offspring begging can link the two different approaches. Using theoretical and experimental analyses, we show that when offspring control provisioning, prenatal effects primarily serve the parent's interests: Selection on parents drives coadaptation of parent and offspring traits. In contrast, when parents control provisioning, prenatal effects primarily serve the offspring's interests: Selection on the offspring drives coadaptation of parent and offspring traits. Parent-offspring conflict may thus be responsible for the selective forces that generate parent-offspring coadaptation.},
author = {Hinde, Camilla a and Johnstone, Rufus A and Kilner, Rebecca M},
doi = {10.1126/science.1186056},
file = {:D$\backslash$:/Hedjour/s.v/Master/M1/Manchots royaux-InvPar/Biblio/parentalcare/parentalcare/Parent-Offspring Conflict and Coadaptation.pdf:pdf},
issn = {1095-9203},
journal = {Science (New York, N.Y.)},
keywords = {Adaptation,Animal,Animals,Behavior,Biological,Canaries,Canaries: growth \& development,Conflict (Psychology),Cues,Evolution,Feeding Behavior,Female,Genetic,Genetic Fitness,Maternal Behavior,Nesting Behavior,Selection},
month = mar,
number = {5971},
pages = {1373--6},
pmid = {20223985},
title = {{Parent-offspring conflict and coadaptation.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20223985},
volume = {327},
year = {1969}
}
