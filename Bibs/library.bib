keywords = {Discussion},
mendeley-tags = {Discussion},
number = {1},
pages = {7 -- 14},
title = {{Seasonal Effects of Timing and Reproduction in the King Penguin: A Unique Breeding Cycle}},
url = {http://www.jstor.org/stable/3676955},
volume = {27},
year = {1996}
}
@article{McNamara1999,
abstract = {Evolutionary game theory is concerned with the evolutionarily stable outcomes of the process of natural selection. The theory is especially relevant when the fitness of an organism depends on the behaviour of other members of its population. Here we focus on the interaction between two organisms that have a conflict of interest. The standard approach to such two-player games is to assume that each player chooses a single action and that the evolutionarily stable action of each player is the best given the action of its opponent. We argue that, instead, most two-player games should be modelled as involving a series of interactions in which opponents negotiate the final outcome. Thus we should be concerned with evolutionarily stable negotiation rules rather than evolutionarily stable actions. The evolutionarily stable negotiation rule of each player is the best rule given the rule of its opponent. As we show, the action chosen as a result of the negotiation is not the best action given the action of the opponent. This conclusion necessitates a fundamental change in the way that evolutionary games are modelled.},
author = {McNamara, J.M. and Gasson, C. E and Houston, A.I.},
doi = {10.1038/43869},
file = {:D$\backslash$:/Hedjour/s.v/Master/M1/Manchots royaux-InvPar/Biblio/parentalcare/parentalcare/McNamara, Gasson, Houston - 1999 - Nature.pdf:pdf},
issn = {0028-0836},
journal = {Nature},
keywords = {Animals,Biological,Evolution,Feeding Behavior,Female,Game Theory,Intro,Male,Models,Reproduction},
mendeley-tags = {Intro},
month = sep,
number = {6751},
pages = {368--71},
pmid = {10517633},
title = {{Incorporating rules for responding into evolutionary games.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10517633},
